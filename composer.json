{
  "name": "tyler-sommer/nice",
  "description": "A nice PHP microframework",
  "license": "MIT",
  "require": {
    "php": ">=5.3.3",
    "twig/twig": "~1.15",
    "symfony/debug": "~2.4",
    "symfony/event-dispatcher": "~2.4",
    "symfony/http-foundation": "~2.4",
    "symfony/http-kernel": "~2.4",
    "symfony/console": "~2.4",
    "symfony/process": "~2.4",
    "symfony/yaml": "~2.4",
    "nikic/fast-route": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.7"
  },
  "autoload": {
    "psr-4": {
      "TylerSommer\\Nice\\Tests\\": "tests/",
      "TylerSommer\\Nice\\": "src/"
    }
  },
  "extras": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
